  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "rollup-plugin-typescript2": "^0.35.0",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^3.2.0",
    "vue-tsc": "^1.8.3"
